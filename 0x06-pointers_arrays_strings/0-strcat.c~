#include "main.h"
#include <stddef.h>
#include <string.h>
/**
* _strca - concatenates two strings
*
* Return: char.
*/

char *_strcat(char *dest, char *src){

char *dest_end = dest;

/* Find the end of the destination string*/
while (*dest_end) {
dest_end++;
}

/* Copy the source string to the end of the destination string*/
while (*src) {
*dest_end++ = *src++;
}

/* Add the null terminator*/
*dest_end = '\0';

return dest;
}
